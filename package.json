{
  "name": "tweet-a-gist",
  "version": "1.0.0",
  "description": "Processes location updates and propagates them to GH and Twitter",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "xo && nyc ava && nyc report --reporter=text-lcov > coverage.lcov"
  },
  "engines": {
    "node": "^6.6.0",
    "npm": "^3.10.8",
    "yarn": "^0.15.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/meeDamian/gist-to-twitter.git"
  },
  "author": "Damian Mee <mee.damian@gmail.com> (https://meedamian.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meeDamian/gist-to-twitter/issues"
  },
  "homepage": "https://github.com/meeDamian/gist-to-twitter#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "coveralls": "^2.11.14",
    "nyc": "^8.3.2",
    "xo": "^0.17.0"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "country-emoji": "^1.2.0",
    "express": "^4.14.0",
    "mee": "^0.11.3",
    "pg-promise": "^5.3.4",
    "request": "^2.75.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node"
    ],
    "space": true,
    "rules": {
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ]
    }
  }
}
